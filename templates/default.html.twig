{% extends mag('P_LOGGED_IN') ? 'base.html.twig' : 'extern/layout.html.twig' %}

{% block styles %}
	{% if not mag('P_LOGGED_IN') %}
		{{ css_asset('extern') }}
	{% endif %}
{% endblock %}

{% block titel %}{{ content.titel }}{% endblock %}

{% block content %}{{ content.toString | raw }}{% endblock %}

{% block modal %}
	{% if modal is defined %}
		{{ modal.toString | raw }}
	{% else %}
		<div id="modal" tabindex="-1"></div>
	{% endif %}
{% endblock %}
